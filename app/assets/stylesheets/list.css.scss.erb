
.checkmark {
  width: 80%;
  height: 600px;
  margin-right: auto;
  margin-left: auto;
  margin-top: 70px;
  background: url(' <%= asset_path("check-mark-10-xxl-gold.png") %> ');
  background-size: 100%;
  background-repeat: no-repeat;
  background-position: center center;
  background-color: #393939;
}

.todolist {
  padding: 0px;
  border: 0px;
}

table {
  width: 100%;
}

.tablerows {
  height: 20px;
  font-family: 'Happy Monkey', cursive;
}

h1 {
  text-align: left;
}

.tableheadings {
  height: 30px;
  background-color: #6e6e6e;
  color: #ff282d;
  padding: 10px;
}

.description {
  width: 50%;
  text-align: left;
  padding: 0 0 0 15px;
  margin: 0 0 0 0;
  font-size: 150%;
}

.text_field {
  font-family: 'Happy Monkey', cursive;
  font-size: 125%;
  color: black;
  text-align: left;
}

.days_old {
  width: 25%;
  text-align: center;
  font-size: 150%;
}

.completed {
  width: 25%;
  text-align: center;
  color: #393939;
}

.completed a {
  color: #393939;
  font-family: 'Josefin Sans', sans-serif;
}

.centerlist {
  width: 80%;
  margin-left: auto;
  margin-right: auto;
}

.centerlist h1 {
  text-align: left;
}

.cutoff {
  padding: 10px;
}

.listdel {
  padding: 0 0 0 15px;
}

.listlist {
  font-size: 125%;
  font-family: 'Happy Monkey', cursive;
}

.todolist {
  width: 60%;
  margin-right: auto;
  margin-left: auto;
  font-family: 'Happy Monkey', cursive;
}

.list_form {
  width: 50%;
  margin-right: auto;
  margin-left: auto;
  font-family: 'Happy Monkey', cursive;
}